            if (menuParam.children && menuParam.children.length > 0) {\n                        _addMenu(menu.subs, menuParam.children);\n                    }\n                    menuList.push(menu);\n                }\n            }\n        }\n    },\n    actions: {\n        add_Menus({ commit }, param) {\n            commit('add_Menus', param)\n        }\n    }\n}\nexport default addMenus"]}]}